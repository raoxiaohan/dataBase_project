page {
    height: 100%;
    background: #f4f4f4
}

.container {
    padding: .533333rem
}

.borderBo {
    position: relative
}

.borderBo:before {
    content: '';
    width: 200%;
    height: 0;
    position: absolute;
    left: 0;
    bottom: 0;
    border-bottom: 1px solid #d6d6d6;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0
}

.borderTop {
    position: relative
}

.borderTop:before {
    content: '';
    width: 200%;
    height: 0;
    position: absolute;
    left: 0;
    top: 0;
    border-bottom: 1px solid #d6d6d6;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0
}

.pannel {
    margin-bottom: .533333rem;
    padding-top: .533333rem;
    position: relative;
    border-radius: 10px
}

.pannel:before {
    content: '';
    width: 200%;
    height: 200%;
    position: absolute;
    left: 0;
    top: 0;
    border: 1px solid #cacaca;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    border-radius: 10px;
    z-index: -1;
    background-color: #fff
}

.dateNum {
    padding: 0 1.066667rem 0 .906667rem;
    height: 2.24rem;
    line-height: 2.24rem;
    -ms-flex-align: center;
    -ms-flex-pack: justify
}

.dateNum,
.dateNum .dateOuter {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between
}

.dateNum .dateOuter {
    -webkit-box-flex: 1;
    -ms-flex: 1;
    flex: 1;
    -ms-flex-align: center;
    -ms-flex-pack: justify
}

.dateNum .dateOuter .date text {
    font-size: 30rpx

}

.dateNum .dateOuter .date text.small {
    color: #aaa;
    font-size: 25rpx;
    margin-left: 1rem
}

.dateNum .num {
    font-size: 30rpx;
    width: 6.5em;
    text-align: center
}

.flight {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -ms-flex-direction: column;
    flex-direction: column
}

.flight .goFlight {
    position: relative;
    padding-left: .8rem;
    padding-right: .8rem;
}

.flight .goFlight:before {
    content: '';
    width: 200%;
    height: 0;
    position: absolute;
    left: 0;
    bottom: 0;
    border-bottom: 1px solid #d6d6d6;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0
}

.flight .backFlight {
    position: relative;
    padding-top: .533333rem;
    padding-left: .8rem;
    padding-right: .8rem
}

.flight .backFlight:before {
    content: '';
    width: 200%;
    height: 0;
    position: absolute;
    left: 0;
    bottom: 0;
    border-bottom: 1px solid #d6d6d6;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0
}

.flight .flightDate {
    -webkit-box-orient: horizontal;
    -ms-flex-direction: row;
    flex-direction: row;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between
}

.flight .flightDate,
.flight .flightDate view {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-direction: normal
}

.flight .flightDate view {
    -webkit-box-orient: vertical;
    -ms-flex-direction: column;
    flex-direction: column
}

.flight .flightDate view text {
    font-size: 27rpx
}

.flight .flightDate view .time {
    font-size: 50rpx
}

.flight .flightDate view .date {
    color: #aaa;
    text-align: center;
    margin-bottom: .266667rem;
    font-size: 30rpx
}

.flight .flightDate view .flightPic {
    width: 8.666667rem;
    height: 1.066667rem
}

.flight .flightLine {
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between
}

.flight .flightLine,
.flight .flightLine .viewLeft {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center
}

.flight .flightLine .viewLeft {
    -webkit-box-flex: 1;
    -ms-flex: 1;
    flex: 1
}

.flight .flightLine image {
    width: .666667rem;
    height: .533333rem;
    margin-right: .266667rem;
    display: block
}

.flight .flightLine .go {
    background-color: #ff7f00;
    color: #fff;
    font-size: 28rpx;
    width: 2.293333rem;
    height: 1.226667rem;
    line-height: 1.226667rem;
    text-align: center;
    display: block
}

.flight .airport {
    color: #aaa
}

.flight .airport text {
    font-size: 28rpx
}

.flight .airport .small {
    font-size: 22rpx
}

.flight .flightDetail {
    height: 2.133333rem;
    line-height: 2.133333rem;
    margin-top: .213333rem;
    position: relative
}

.flight .flightDetail:before {
    content: '';
    width: 200%;
    height: 0;
    position: absolute;
    left: 0;
    top: 0;
    border-bottom: 1px dashed #d6d6d6;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    border-radius: 10px
}

.flight .flightDetail text {
    font-size: 25rpx;
    color: #9caabc
}

.flight .flightDetail .flightName {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden
}

.flight .flightDetail .flightName image {
    width: .64rem;
    height: .506667rem
}

.flight .flightDetail .flightType {
    width: 60%;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden
}

.flight .flightDetail .vertical {
    width: 10%;
    text-align: center
}

.flight .changeFlight {
    font-size: 30rpx;
    text-align: center;
    color: #aaa;
    height: 1.866667rem;
    line-height: 1.866667rem;
    padding-left: .8rem;
    padding-right: .8rem
}
.go{
    width: 6rem;
}
